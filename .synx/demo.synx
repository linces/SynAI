{
  "type": "Program",
  "id": "acb48ffa-9c91-4e0a-ae45-9276f67282f0",
  "declarations": [
    {
      "type": "Orchestrator",
      "id": "9101a385-eaed-4cc6-b85a-d1f6f808d79a",
      "name": "demo_advanced",
      "blocks": [
        {
          "type": "AgentsBlock",
          "id": "ebe1718c-eef4-4022-b8c6-0f7d989c6b7b",
          "agents": [
            {
              "type": "Agent",
              "id": "analyzer",
              "agent_type": "LLM",
              "properties": {
                "model": "claude-3-opus-20240229",
                "capabilities": [
                  "reason",
                  "analyze"
                ],
                "endpoint": "https://api.anthropic.com/v1"
              }
            },
            {
              "type": "Agent",
              "id": "coder",
              "agent_type": "LLM",
              "properties": {
                "model": "grok-beta",
                "capabilities": [
                  "code",
                  "generate"
                ],
                "endpoint": "https://api.x.ai/v1"
              }
            },
            {
              "type": "Agent",
              "id": "summarizer",
              "agent_type": "LLM",
              "properties": {
                "model": "claude-3-sonnet-20240229",
                "capabilities": [
                  "summarize",
                  "reason"
                ],
                "endpoint": "https://api.anthropic.com/v1"
              }
            },
            {
              "type": "Agent",
              "id": "visualizer",
              "agent_type": "Vision",
              "properties": {
                "model": "gpt-4-vision-preview",
                "capabilities": [
                  "image_analyze",
                  "describe"
                ],
                "endpoint": "https://api.openai.com/v1"
              }
            }
          ]
        },
        {
          "type": "Workflow",
          "id": "736d4b69-2410-47eb-bc90-4ea98de33a10",
          "name": "data_pipeline",
          "statements": [
            {
              "type": "Intent",
              "id": "8ad2b694-f913-4026-b3c5-a99aa8d7f932",
              "agent": "analyzer",
              "name": "analyze_data",
              "input": "data.txt",
              "output": "analysis.json"
            },
            {
              "type": "Connect",
              "id": "c4b0b2ed-b4c4-4946-a1a2-240e87b0b503",
              "from": "analyzer",
              "to": "visualizer",
              "options": {
                "async": false,
                "timeout": 10,
                "transform": "json_to_image"
              }
            },
            {
              "type": "Connect",
              "id": "e8686941-f4ea-43c8-b04e-728173f1f642",
              "from": "visualizer",
              "to": "coder",
              "options": {
                "retry": 3,
                "filter": "success"
              }
            },
            {
              "type": "Intent",
              "id": "46ee5b67-02d7-4b53-8b39-f840fca106bb",
              "agent": "coder",
              "name": "generate_code",
              "input": "analysis.json",
              "output": "script.py"
            },
            {
              "type": "Connect",
              "id": "d7361c5e-5734-4fd3-8dbc-9a943d6807f0",
              "from": "coder",
              "to": "summarizer",
              "options": {
                "async": false,
                "timeout": 5
              }
            },
            {
              "type": "Intent",
              "id": "1bc671d3-e7d8-48ec-8697-7f214fd5ba4e",
              "agent": "summarizer",
              "name": "generate_summary",
              "input": "script.py",
              "output": "final_report.md"
            }
          ]
        }
      ]
    },
    {
      "type": "Run",
      "id": "e5ac476c-356d-418e-9961-bd84710886f7",
      "orchestrator": "demo_advanced",
      "workflow": "data_pipeline"
    }
  ],
  "warnings": []
}